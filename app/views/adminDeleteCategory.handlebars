{{>adminNavbar}}

<br>
<div class="container">

  <div class="row justify-content-center">
    <div class="col-md-5">
  
      <br>
      <ul class="list-group">
        <li class="list-group-item active text-center">Existing Categories<p class="card-text"></p></li>
        {{#each categories}}
        <li class="list-group-item"><p class="card-text">{{this.categories}}</p></li>
        {{/each}}
      </ul>
      <br><br><br>
    </div>
  </div>
  <div class="row justify-content-center">
    <div class="col-md-6 ">
      <p class="text-center text-primary lead"> 
        {{#if success}}
          <br>
          {{this.success}}
          <br>
        {{/if}}
      </p>
      <form id="deleteFormId">
        <div class="form-group">
    <label for="dropDownSelect">Remove Category</label>
    <select class="form-control" id="dropDownSelect">
      {{#each categories}}
        <option>
          {{this.categories}}
        </option>
      {{/each}}
    </select>
  </div>
        <button type="submit" class="btn btn-default" id="submitbtnId">Submit</button>
      </form>
    </div>
  </div>
</div>
<script src="../../js/admin-delete.js"></script>